<h1>All Contacts</h1>

<ul>
<% Contact.all.each do |contact| %>
  <li class="details"><%= contact.first_name %> <%= contact.last_name %> <%= contact.email %> <%= contact.note %></li>
    <a href="/contacts/<%= contact.id %>/edit">Edit</a> |  <a href="/contacts/<%= contact.id %>">Delete</a></li>

<% end %>
</ul>
